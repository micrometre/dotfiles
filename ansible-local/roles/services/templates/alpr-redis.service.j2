[Unit]
Description=Alprd and Redis Docker containers
After=docker.service
Requires=docker.service


[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/bin/bash -c "docker compose -f /home/vagrant/microanpr/docker-compose.yml up --detach redis"
ExecStop=/bin/bash -c "docker compose -f  /home/ubunut/microanpr/docker-compose.yml stop redis"
ExecReload=/bin/bash -c "docker compose -f  /home/vagrant/microanpr/docker-compose.yml down -v redis"