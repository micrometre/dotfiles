<network>
  <name>{{ libvirt_default_network.name }}</name>
  <uuid>{{ libvirt_default_network.uuid }}</uuid>
  <forward mode='{{ libvirt_default_network.forward_mode }}'>
    <nat>
      <port start='1024' end='65535'/>
    </nat>
  </forward>
  <bridge name='{{ libvirt_default_network.bridge_name }}' stp='{{ libvirt_default_network.bridge_stp }}' delay='{{ libvirt_default_network.bridge_delay }}'/>
  <mac address='{{ libvirt_default_network.mac_address }}'/>
  <ip address='{{ vm_network_range.gateway }}' netmask='{{ vm_network_range.netmask }}'>
    <dhcp>
      <range start='{{ vm_network_range.dhcp_start }}' end='{{ vm_network_range.dhcp_end }}'/>
    </dhcp>
  </ip>
</network>